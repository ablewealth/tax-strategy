  "keywords": ["tax", "optimization", "wealth-management", "financial-planning", "react"],
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "chart.js": "^4.4.4",
    "react": "^18.3.1",
